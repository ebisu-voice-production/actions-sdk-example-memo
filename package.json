{
  "name": "actions-sdk-example-memo",
  "version": "1.0.0",
  "description": "An Example of Actions on Google by Firebase Cloud Functions",
  "main": "src/index.js",
  "scripts": {
    "firebase": "firebase",
    "start": "npm run shell",
    "shell": "firebase functions:shell",
    "test": "npm run eslint",
    "eslint": "eslint src",
    "deploy": "firebase deploy --only functions",
    "deploy-action": "gactions update --action_package action.json --project ${PROJECT}"
  },
  "engines": {
    "node": ">=6.11.5"
  },
  "dependencies": {
    "actions-on-google": "^2.0.1",
    "firebase-admin": "^5.11.0",
    "firebase-functions": "^1.0.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "firebase-tools": "^3.18.2"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 2015
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "off"
    }
  },
  "keywords": [
    "Actions on Google",
    "Cloud Functions for Firebase"
  ],
  "license": "MIT"
}
